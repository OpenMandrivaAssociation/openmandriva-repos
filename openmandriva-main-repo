[openmandriva-@DIST_ARCH@]
name=OpenMandriva $releasever - @DIST_ARCH@
#baseurl=https://mirrors.kernel.org/openmandriva/distrib/$releasever/@DIST_ARCH@/media/core/release/
#metalink=https://mirrors.openmandriva.org/metalink?distrib=openmandriva-$releasever&arch=@DIST_ARCH@&section=core&repo=release
mirrorlist=https://www.openmandriva.org/mirrorlist/?release=$releasever&arch=@DIST_ARCH@&section=core&repo=release
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-OpenMandriva
failovermethod=priority
enabled=1

