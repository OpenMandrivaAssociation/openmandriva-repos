[openmandriva-@DIST_ARCH@]
name=OpenMandriva $releasever - @DIST_ARCH@
baseurl=http://abf-downloads.openmandriva.org/$releasever/repository/@DIST_ARCH@/main/release/
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-OpenMandriva
enabled=1

[updates-@DIST_ARCH@]
name=OpenMandriva $releasever - @DIST_ARCH@ - Updates
baseurl=http://abf-downloads.openmandriva.org/$releasever/repository/@DIST_ARCH@/main/updates/
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-OpenMandriva
enabled=1

[testing-@DIST_ARCH@]
name=OpenMandriva $releasever - @DIST_ARCH@ - Test Updates
baseurl=http://abf-downloads.openmandriva.org/$releasever/repository/@DIST_ARCH@/main/testing/
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-OpenMandriva
enabled=0

[openmandriva-@DIST_ARCH@-debuginfo]
name=OpenMandriva $releasever - @DIST_ARCH@ - Debug
baseurl=http://abf-downloads.openmandriva.org/$releasever/repository/@DIST_ARCH@/debug_main/release/
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-OpenMandriva
enabled=0

[updates-@DIST_ARCH@-debuginfo]
name=OpenMandriva $releasever - @DIST_ARCH@ - Updates - Debug
baseurl=http://abf-downloads.openmandriva.org/$releasever/repository/@DIST_ARCH@/debug_main/updates/
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-OpenMandriva
enabled=0

[testing-@DIST_ARCH@-debuginfo]
name=OpenMandriva $releasever - @DIST_ARCH@ - Test Updates - Debug
baseurl=http://abf-downloads.openmandriva.org/$releasever/repository/@DIST_ARCH@/debug_main/testing/
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-OpenMandriva
